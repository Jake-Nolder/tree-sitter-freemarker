{
  "name": "freemarker",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "directive_definition"
        },
        {
          "type": "SYMBOL",
          "name": "function_definition"
        }
      ]
    },
    "function_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<#function"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "</#function>"
        }
      ]
    },
    "directive_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "<#"
            },
            {
              "type": "STRING",
              "value": "</#"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "STRING",
              "value": "/>"
            }
          ]
        }
      ]
    },
    "_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "assign"
        },
        {
          "type": "STRING",
          "value": "attempt"
        },
        {
          "type": "STRING",
          "value": "autoesc"
        },
        {
          "type": "STRING",
          "value": "break"
        },
