              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_expression"
              }
            ]
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "alphnumerical": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9]+"
    },
    "asciiSymbol": {
      "type": "PATTERN",
      "value": "[\\!-\\@\\[-\\`\\{-\\~]"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
