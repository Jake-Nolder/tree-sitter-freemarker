        {
          "type": "SYMBOL",
          "name": "ftlTag_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_block"
          }
        },
        {
          "type": "SYMBOL",
          "name": "ftlTag_end"
        }
      ]
    },
    "ftlTag_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "<#"
            },
            {
              "type": "STRING",
              "value": "</#"
