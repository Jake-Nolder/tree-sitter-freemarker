{
  "name": "freemarker",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "interpolation_definition"
        },
        {
          "type": "SYMBOL",
          "name": "ftlTag_definition"
        },
        {
          "type": "SYMBOL",
          "name": "comments_definition"
        },
        {
          "type": "SYMBOL",
          "name": "_parameters"
        }
      ]
    },
    "interpolation_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "ftlTag_definition"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "comments_definition": {
      "type": "SEQ",
